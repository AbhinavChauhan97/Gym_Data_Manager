<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <Transition
        app:constraintSetEnd="@+id/end3"
        app:constraintSetStart="@+id/start3"
        app:duration="5000">
        <OnClick app:targetId="@layout/memberimage_cardview" />
    </Transition>

    <ConstraintSet android:id="@+id/start3">
        <Constraint android:id="@+id/member_feerecords_recyclerview" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end3">
        <Constraint
            android:id="@+id/member_feerecords_recyclerview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_constraintTop_toTopOf="parent" />
    </ConstraintSet>
</MotionScene>
